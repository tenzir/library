// Poor man's proxy log detection
if this.has("requestmethod") {
  zscaler::ocsf::zia::proxy
} else {
  // Base Event
  this = {event: this}
  ocsf.activity_id = 0
  ocsf.category_uid = 0
  ocsf.class_uid = 0
  ocsf.type_uid = 0
  ocsf.time = now()
  ocsf.metadata = {
    product: {
      name: "Unknown",
      vendor_name: "Zscaler",
    },
    version: "1.6.0",
  }
  ocsf.severity_id = 0
  this = {...ocsf, unmapped: event}
  ocsf::derive
}
